    "**/*.{js,jsx,vue,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "koa": "^2.5.2",
