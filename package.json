    "markdown-it-emoji": "^1.4.0",
    "markdown-it-highlightjs": "^3.0.0",
    "markdown-it-kbd": "^2.0.0",
    "markdown-it-mark": "^2.0.0",
    "markdown-it-toc-and-anchor": "^4.2.0",
    "vee-validate": "^2.1.6",
    "vue-i18n": "^8.15.3",
    "vue-photo-preview-op": "0.0.2"
  },
  "devDependencies": {
    "css-loader": "4.3.0",
    "husky": "^7.0.4",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lint-staged": "^12.1.7",
    "nodemon": "^1.11.0",
    "nuxt": "^2.0.0",
    "prettier": "^2.5.1",
    "request": "^2.88.2"
