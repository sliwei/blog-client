  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build && node ./script/upload.js",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
